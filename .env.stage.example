# Staging Environment Configuration
# This file is used in CI/CD - secrets injected from GitHub Secrets

# Environment
ENVIRONMENT=stage

# Secrets (Injected from GitHub Secrets)
YOUTUBE_API_KEY_STAGE=${YOUTUBE_API_KEY_STAGE}
DATABRICKS_TOKEN_STAGE=${DATABRICKS_TOKEN_STAGE}
DATABRICKS_HOST_STAGE=${DATABRICKS_HOST_STAGE}
DATABRICKS_HTTP_PATH_STAGE=${DATABRICKS_HTTP_PATH_STAGE}

# Database Connections
POSTGRES_HOST_STAGE=${POSTGRES_HOST_STAGE}
POSTGRES_PORT_STAGE=5432
POSTGRES_DB_STAGE=stage_db
POSTGRES_USER_STAGE=${POSTGRES_USER_STAGE}
POSTGRES_PASSWORD_STAGE=${POSTGRES_PASSWORD_STAGE}

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# DLT
DLT_PROJECT_DIR=.dlt
DLT_PIPELINE_DIR=.dlt/pipelines
