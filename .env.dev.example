# Development Environment Configuration
# Copy this file to .env and fill in actual values

# Environment
ENVIRONMENT=dev

# Secrets (GitHub Secrets in CI/CD)
YOUTUBE_API_KEY_DEV=your-youtube-api-key-here
DATABRICKS_TOKEN_DEV=your-databricks-token-here
DATABRICKS_HOST_DEV=your-databricks-host-here
DATABRICKS_HTTP_PATH_DEV=your-databricks-http-path-here

# Database Connections
POSTGRES_HOST_DEV=localhost
POSTGRES_PORT_DEV=5432
POSTGRES_DB_DEV=dev_db
POSTGRES_USER_DEV=dev_user
POSTGRES_PASSWORD_DEV=dev_password

# Logging
LOG_LEVEL=DEBUG
LOG_FORMAT=json

# DLT
DLT_PROJECT_DIR=.dlt
DLT_PIPELINE_DIR=.dlt/pipelines
