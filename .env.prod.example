# Production Environment Configuration
# This file is used in CI/CD - secrets injected from GitHub Secrets

# Environment
ENVIRONMENT=prod

# Secrets (Injected from GitHub Secrets)
YOUTUBE_API_KEY_PROD=${YOUTUBE_API_KEY_PROD}
DATABRICKS_TOKEN_PROD=${DATABRICKS_TOKEN_PROD}
DATABRICKS_HOST_PROD=${DATABRICKS_HOST_PROD}
DATABRICKS_HTTP_PATH_PROD=${DATABRICKS_HTTP_PATH_PROD}

# Database Connections
POSTGRES_HOST_PROD=${POSTGRES_HOST_PROD}
POSTGRES_PORT_PROD=5432
POSTGRES_DB_PROD=prod_db
POSTGRES_USER_PROD=${POSTGRES_USER_PROD}
POSTGRES_PASSWORD_PROD=${POSTGRES_PASSWORD_PROD}

# Logging
LOG_LEVEL=WARNING
LOG_FORMAT=json

# DLT
DLT_PROJECT_DIR=.dlt
DLT_PIPELINE_DIR=.dlt/pipelines

# Monitoring
SENTRY_DSN=${SENTRY_DSN}
